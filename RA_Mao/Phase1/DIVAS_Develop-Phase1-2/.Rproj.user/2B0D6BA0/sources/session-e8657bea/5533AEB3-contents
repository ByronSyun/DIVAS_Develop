#install.packages("naniar")
#install.packages("visdat")

library(naniar)
library(visdat)

# Sample data frame with missing values
data <- data.frame(
  Name = c("Alice", "Bob", NA, "David"),
  Age = c(24, NA, 22, 29),
  Score = c(88, 92, NA, 85)
)

# Summarize missing values
print(colSums(is.na(data)))

